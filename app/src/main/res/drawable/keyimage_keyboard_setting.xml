        <CategoryColorSelected android:resource="@color/colorAccent" />

        <!-- 変換候補 -->
        <CandidateColor android:resource="@color/colorForegroundPrimary" />
        <CandidateHintColor android:resource="@color/colorAccent" />
        <CandidateSymbolColor android:resource="@color/colorAccent" />
        <CandidateNumberColor android:resource="@color/colorAccent" />

        <!-- 外部変換 -->
        <WebApiButtonColor android:resource="@color/colorAccent" />
        <WebApiCandidateColor android:resource="@color/colorAccent" />

        <!-- 変換候補なし -->
        <NoCandidateColor android:resource="@color/colorAccent" />

        <!-- 変換候補右上の展開ボタン -->
        <CandUpDownColor android:resource="@color/colorForegroundHint" />
        <CandUpDownBackgroundColor android:resource="@color/colorBackground" />
    </Color>
