        <SlidePopupBackgroundNormal android:resource="@color/colorBackgroundAlternate" />
        <SlidePopupBackgroundFocused android:resource="@color/colorAccent" />

        <!-- キーボード押下時ポップアップ -->
        <KeyPreviewColor android:resource="@color/colorForegroundAlternate" />

        <!-- 記号入力上タブ -->
        <TabSelectColor android:resource="@color/colorAccent" />
        <TabNoSelectColor android:resource="@color/colorForegroundSecondary" />
        <TabDisableColor android:resource="@color/colorForegroundHint" />

        <!-- 記号入力下タブ -->
        <CategoryColorNormal android:resource="@color/colorForegroundSecondary" />
