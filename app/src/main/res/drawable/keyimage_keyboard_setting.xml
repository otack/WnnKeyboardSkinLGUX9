<?xml version="1.0" encoding="utf-8"?>
<Skindata xmlns:android="http://schemas.android.com/apk/res/android">
    <String>
        <TargetVersion android:value="2.30" />
    </String>
    <Color>
        <!-- メニューバー -->
        <MenuBarBackground android:resource="@color/colorBackgroundStatic" />
        <MenuBarBackgroundSelected android:resource="@color/colorBackgroundStaticSelected" />

        <!-- 文字入力キー文字色 -->
        <KeyColor android:resource="@color/colorForegroundPrimary" />
